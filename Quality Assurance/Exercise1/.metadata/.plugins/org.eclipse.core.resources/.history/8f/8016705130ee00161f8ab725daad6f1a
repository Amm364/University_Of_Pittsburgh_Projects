
public class Cat {
	String name;
	Boolean rented;
	double cost;
	int id;
	int currentDaysRented;
	public Cat(String catName,double c,int cid,int cdr){
		name=catName;
		rented=false;
		cost=c;
		id=cid;
		currentDaysRented=cdr;
	}
	
	public String rentCat(){
		if (rented){
			return "Cat with ID " + id + " has already been rented";
		}
		else{
			rented=true;
			return "Cat with ID " + id + " is now being rented.";
		}
	}
